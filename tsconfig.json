{
    "compilerOptions": {
        "target": "ES2020",
        "module": "None",
        "lib": [
            "ES2020",
            "DOM",
            "DOM.Iterable"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./static/dist"
    },
    "include": [
        "./static/*.ts"
    ],
    "exclude": []
}